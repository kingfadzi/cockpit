# Backend API base URL - using relative URLs with Vite proxy
# VITE_API_BASE=http://localhost:8080

# Audit API base URL - using relative URLs with Vite proxy  
# VITE_AUDIT_API_BASE=http://localhost:8081

# Vite proxy targets (for development server)
VITE_API_TARGET=http://mars.butterflycluster.com:8080
VITE_AUDIT_API_TARGET=http://mars.butterflycluster.com:8081

# Docker Compose environment variables (for local development)
BACKEND_BASE_URL=http://localhost:8080
AUDIT_API_BASE_URL=http://localhost:8081

# Set to 1 to use built-in mock data instead of calling backend
VITE_USE_MOCK=0
